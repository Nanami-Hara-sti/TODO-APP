{
    "name": "B-Container",
    "dockerComposeFile": "../../docker-compose.yml", 
    "service": "backend",
    "workspaceFolder": "/workspace/backend",
    "forwardPorts": [8000],
    "portsAttributes": {
        "8000": {
            "label": "FastAPI Backend",
            "protocol": "http"
        }
    },
    "shutdownAction": "stopCompose",
    
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "dbaeumer.vscode-eslint",
                "esbenp.prettier-vscode"
            ]
        }
    }
}